{
  "name": "bunnymq",
  "version": "2.3.2",
  "description": "BunnyMq is a RabbitMq wrapper",
  "keywords": [
    "rabbitmq",
    "broker",
    "amqp",
    "taskQueue",
    "message",
    "publish",
    "subscribe"
  ],
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "lint": "./node_modules/.bin/eslint .",
    "cover": "CONSOLE_LOGGING=false node_modules/.bin/istanbul cover ./node_modules/mocha/bin/_mocha -- test --recursive --timeout=20000 --exit",
    "test": "npm run lint && npm run cover",
    "doc": "jsdoc -d ./docs --readme README.md --package package.json --template node_modules/minami src/*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dial-once/node-bunnymq.git"
  },
  "author": "Dial Once",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dial-once/node-bunnymq/issues"
  },
  "homepage": "https://github.com/dial-once/node-bunnymq#readme",
  "dependencies": {
    "amqplib": "^0.4.1",
    "deserialize-error": "0.0.3",
    "dotenv": "^2.0.0",
    "serialize-error": "^2.1.0",
    "uuid": "^3.0.1"
  },
  "devDependencies": {
    "assert": "1.3.0",
    "child-process-promise": "^2.2.0",
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "istanbul": "^1.1.0-alpha.1",
    "mocha": "^4.0.1",
    "minami": "1.1.1"
  },
  "engines": {
    "node": ">=6"
  }
}
